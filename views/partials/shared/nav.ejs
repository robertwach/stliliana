 <!-- Main Header-->
 <header class="main-header">
        
    <!-- Header Top -->
    <div class="header-top">
        <div class="auto-container clearfix">
            <!--Top Left-->
            <div class="top-left pull-left">
                <ul class="links-nav clearfix">
                    <li><span class="icon fa fa-envelope-o"></span><a href="#"> <%= email  %> </a></li>
                    <li><span class="icon fa fa-phone"></span><a href="#">Call Us Now : <%= phoneNumber %> </a></li>
                </ul>
            </div>
            
            <!--Top Right-->
            <div class="top-right pull-right">
                <ul class="links-nav clearfix">
                  
                    <li>
                        <a href="/location">
                        <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-geo-alt-fill" viewBox="0 0 16 16">
                            <path d="M8 16s6-5.686 6-10A6 6 0 0 0 2 6c0 4.314 6 10 6 10zm0-7a3 3 0 1 1 0-6 3 3 0 0 1 0 6z"/>
                          </svg>
                    </a>
                </li>
                </ul>
            </div>
        </div>
    </div>
    <!-- Header Top End -->
    
    <!--Header-Upper-->
    <div class="header-upper">
        <div class="auto-container">
                
            <div class="logo-outer logo-container">
                <div class="logo"><a href="/">
                    <img src="/logo.jpeg" width="60" height="60" alt="mob" title="">
                </a></div>
            </div>
                
            <div class="nav-outer clearfix">
            
                <!-- Main Menu -->
                <nav class="main-menu">
                    <div class="navbar-header">
                        <!-- Toggle Button -->    	
                        <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                        </button>
                    </div>
                    
                    <div class="navbar-collapse collapse clearfix">
                        <!--Left Nav-->
                        <ul class="navigation left-nav clearfix">
                            <li class="<% if (page === '/') { %> current <% } %>">
                            <a href="#">Home</a>
                               
                            </li>
                            <li  class="<% if (page === '/about-us') { %> current <% } %>"><a href="/about">Our school</a></li>
                            <li  class="<% if (page === '/newsletters') { %> current <% } %>"><a href="/newsletters">News letters</a></li>
                        </ul>
                        <!--Right Nav-->
                        <ul class="navigation right-nav clearfix">
                            <li  class="<% if (page === '/gallery') { %> current <% } %>">
                                <a href="/gallery">Gallery</a></li>

                            <li  class="<% if (page === '/blog') { %> current <% } %>">
                                <a href="/blog">Blog</a> </li>
                            <li  class="<% if (page === '/contact-us') { %> current <% } %>">
                                <a href="/contact-us">Contact Us</a></li>
                        </ul>
                    </div>
                </nav>
                
               
                
            </div>
        </div>
    </div>
    <!--End Header Upper-->
    
    <!--Sticky Header-->
    <div class="sticky-header">
        <div class="auto-container clearfix">
            <!--Logo-->
            <div class="logo pull-left">
                <a href="/" class="img-responsive">
                    <!-- <img src="/images/logo-small.png" alt="" title=""> -->
                    <img width="60px" height="60" src="/logo.jpeg" alt="" title="">
                </a>
            </div>
            
            <!--Right Col-->
            <div class="right-col pull-right">
                <!-- Main Menu -->
                <nav class="main-menu">
                    <div class="navbar-header">
                        <!-- Toggle Button -->    	
                        <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                            <span class="icon-bar"></span>
                            <span class="icon-bar"></span>
                            <span class="icon-bar"></span>
                        </button>
                    </div>
                    
                    <div class="navbar-collapse collapse clearfix">
                        <ul class="navigation clearfix">
                            <li  class="<% if (page === '/') { %> current <% } %>"><a href="/">Home</a>
                            </li>
                            <li  class="<% if (page === '/about-us') { %> current <% } %>">
                                <a href="/about">Our school</a>
                            </li>
                            <li  class="<% if (page === '/newsletter') { %> current <% } %>">
                                <a href="/newsletters">Newsletters</a>
                            </li>
                           
                            <li  class="<% if (page === '/gallery') { %> current <% } %>">
                                <a href="/gallery">Gallery</a>
                            </li>
                            <li  class="<% if (page === '/blog') { %> current <% } %>">
                                <a href="/blog">Blog</a>
                            </li>
                            <li  class="<% if (page === '/contact-us') { %> current <% } %>">
                                <a href="/contact-us">Contact Us</a>
                            </li>
                        </ul>
                    </div>
                </nav><!-- Main Menu End-->
            </div>
            
        </div>
    </div>
    <!--End Sticky Header-->

</header>
<!--End Main Header -->